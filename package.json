{
  "name": "behavior-tree-zmq",
  "version": "0.5.0",
  "description": "Optional dependency of async-behavior-tree that allows live debugging using Groot via zmq.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest --no-cache",
    "jestw": "jest --watch --runInBand --detectOpenHandles",
    "jestc": "jest --clearCache",
    "build": "tsc",
    "tsc": "tsc",
    "clean": "rimraf dist",
    "cleandist": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:esromneb/behavior-tree-zmq.git"
  },
  "keywords": [
    "Behavior Tree",
    "BehaviorTree.CPP",
    "Groot",
    "ZMQ",
    "ZeroMQ"
  ],
  "author": "Benjamin Morse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/esromneb/behavior-tree-zmq/issues"
  },
  "homepage": "https://github.com/esromneb/behavior-tree-zmq#readme",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "jest": "^26.4.2",
    "jest-raw-loader": "^1.0.1",
    "jest-watch-typeahead": "^0.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "zeromq": "^6.0.0-beta.6"
  }
